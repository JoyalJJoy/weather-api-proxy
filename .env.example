# Visual Crossing API Key (get from https://www.visualcrossing.com/account)
VISUAL_CROSSING_API_KEY=your_api_key_here

# Redis connection URL (Railway will auto-provide this)
REDIS_URL=redis://localhost:6379

# Server port (Railway will auto-provide this)
PORT=3000

# Cache TTL in seconds (600 = 10 minutes)
CACHE_TTL=600
